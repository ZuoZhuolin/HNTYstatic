/* pages/EMQ/EMQ.wxss */
.static{
  box-sizing:border-box;
  display:flex;
  justify-content:flex-start;
  align-items:flex-start;
  flex-direction:column; 
  flex-wrap:nowrap; 
  align-content:stretch; 
  width:100%;
  height: 1960rpx; 
  background-color: transparent;
  font-size: 20PX; 
  line-height:40rpx; 
  
}
.instruId{
  box-sizing:border-box;
  display:flex;
  justify-content:flex-start;
  align-items:flex-start;
  flex-direction:row; 
  flex-wrap:wrap; 
  align-content:stretch; 
  margin: 16rpx 0rpx 0rpx 0rpx; 
  width:100%;
  height: 4%; 
  display: -webkit-box;
  display: -moz-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  color: #000000;  
  font-size: 20PX; 
  line-height:40rpx; 
}
.detail{
  box-sizing:border-box;
  display:flex;
  justify-content:flex-start;
  align-items:flex-start;
  flex-direction:row; 
  flex-wrap:wrap; 
  align-content:stretch; 
  margin: 16rpx 0rpx 0rpx 0rpx; 
  width:100%;
  height: 4%; 
  display: -webkit-box;
  display: -moz-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  color: #000000;  
  font-size: 20PX; 
  line-height:40rpx; 
}
.connectButton{
  position: fixed;
  
  bottom: 0;

  width: 100%;
}

